{
  "name": "excel-helper-workers",
  "version": "1.0.0",
  "description": "Background workers for Excel Helper",
  "main": "fileWorker.js",
  "scripts": {
    "start": "node fileWorker.js",
    "start:message": "node messageWorker.js",
    "start:recovery": "node recoveryWorker.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:message\" \"npm run start:recovery\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "bullmq": "^5.4.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "node-fetch": "^3.3.2"
  }
}
