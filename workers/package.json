
{
  "name": "excel-assistant-workers",
  "version": "1.0.0",
  "description": "Worker services for Excel Assistant",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:message": "node --loader ts-node/esm messageWorker.ts",
    "start:file": "node fileWorker.js",
    "start:all": "concurrently \"npm run start:message\" \"npm run start:file\"",
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "bullmq": "^4.14.3",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "concurrently": "^8.2.1",
    "typescript": "^5.2.2"
  },
  "type": "module"
}
